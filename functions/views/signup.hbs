<!DOCTYPE html>
<html lang="en">

    <head>
        {{{headerContents}}}
    </head>
    <script src="scripts/signup.js"></script>
    <body>
        <div class="container">
            <nav class="navbar navbar-expand-lg navbar-light bg-light">
                {{{loginForm}}}
            </nav>
            <h1>Sign Up</h1>
            <div>
                <div class="form-group">
                    <label for="email">Email: </label>
                    <input class="form-control" type="text" id="signup-email" name="email" placeholder="Type in your email"/>
                </div>
                <div class="form-group">
                    <label for="Password">Password: </label>
                    <input id="signup-password" class="form-control" type="password" name="password" placeholder="Type in your password" />
                </div>
                <button id="signup" class="btn btn-primary" type="submit">Submit</button>
            </div>
        </div>
    </body>
</html>